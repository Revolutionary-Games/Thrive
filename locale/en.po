msgstr ". Can"

#: ../src/gui_common/tooltip/ToolTipManager.tscn:4847
msgid "RELEASE_TOXINS_BY_PRESSING"
msgstr "release toxins by pressing"

#: ../src/gui_common/tooltip/ToolTipManager.tscn:4854
msgid "E_DOT"
