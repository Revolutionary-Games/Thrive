void morningskybox_fp (
	float2 uv : TEXCOORD0,
	out float4 colour : COLOR,

	uniform sampler2D tex : register(s0) )
{
	colour = tex2D(tex, uv);

	// blow out the light a bit
	colour *= 1.7;
}
void morningcubemap_fp (
	float3 uv : TEXCOORD0,
	out float4 colour : COLOR,

	uniform samplerCUBE tex : register(s0) )
{
	colour = texCUBE(tex, uv);

	// blow out the light a bit
	colour *= 1.7;
}



