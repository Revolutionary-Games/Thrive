[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn/HelpBox6/MarginContainer" index="0"]
margin_bottom = 59.0
text = "There are many Organelles in the editor for you to evolve, allowing for a wide range of different playstyles."

[node name="HelpBox7" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn" index="6" instance=ExtResource( 73 )]
