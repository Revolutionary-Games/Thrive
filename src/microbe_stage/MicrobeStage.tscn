[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn/HelpBox/MarginContainer" index="0"]
margin_bottom = 173.0
text = "The various compound clouds are:

White – Glucose 
Yellow – Hydrogen Sulfide
Orange - Ammonia 
Purple - Phosphate 
Rust Brown - Iron 

Glucose makes ATP"

[node name="HelpBox2" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn" index="1" instance=ExtResource( 73 )]
margin_top = 193.0
