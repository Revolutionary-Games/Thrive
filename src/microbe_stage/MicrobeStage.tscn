[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn/HelpBox2/MarginContainer" index="0"]
margin_bottom = 78.0
text = "Hydrogen Sulfide can be converted into glucose via chemoplasts and chemosynthesizing proteins Iron can be converted via rusticyanin into ATP."

[node name="HelpBox3" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn" index="2" instance=ExtResource( 73 )]
margin_top = 291.0
