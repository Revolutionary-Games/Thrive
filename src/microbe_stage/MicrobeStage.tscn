margin_bottom = 102.0

[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn/HelpBox4/MarginContainer" index="0"]
margin_bottom = 97.0
text = "You can also engulf cells and bacteria and iron chunks and cell chunks that are smaller than you by pressing G. This will cost additional ATP and will slow you down. Don't forget to press G a second time to stop engulfing."

[node name="HelpBox5" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn" index="4" instance=ExtResource( 73 )]
margin_top = 335.0
margin_bottom = 475.0

[node name="MarginContainer" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn/HelpBox5" index="0"]
