text = "The various compound clouds are:

White â Glucose 
Yellow â Hydrogen Sulfide
Orange - Ammonia 
Purple - Phosphate 
Rust Brown - Iron 

Glucose makes ATP"

[node name="HelpBox2" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn" index="1" instance=ExtResource( 73 )]
margin_top = 193.0
margin_bottom = 276.0

[node name="MarginContainer" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn/HelpBox2" index="0"]
