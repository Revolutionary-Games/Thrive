[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn/HelpBox3/MarginContainer" index="0"]
margin_bottom = 78.0
text = "To reproduce you need to divide each of your organelles into two and then duplicate the DNA in your nucleus. Each organelle needs 2 ammonia and 2 phosphate to split in half."

[node name="HelpBox4" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn" index="3" instance=ExtResource( 73 )]
margin_top = 389.0
margin_bottom = 472.0

[node name="MarginContainer" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn/HelpBox4" index="0"]
margin_bottom = 83.0

[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn/HelpBox4/MarginContainer" index="0"]
margin_bottom = 78.0
text = "But if you survive for twenty generations with 300 population, you are considered to have won, after winning you get a popup and can continue playing as you wish."

[node name="HelpBox5" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/RightColumn" index="4" instance=ExtResource( 73 )]
margin_top = 487.0
