[node name="Label" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn/HelpBox5/MarginContainer" index="0"]
margin_bottom = 135.0
text = "Osmoregulation Now costs ATP, which means the bigger your cell is, the more Mitochondria or Metabolosomes or Rusticyanin (or cytoplasm, which does Glycolysis) you need to avoid losing ATP when you are stationary. It works this way: Osmoregulation costs are ((Hex Amount * 1) per second..."

[node name="HelpBox6" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn" index="5" instance=ExtResource( 73 )]
margin_top = 490.0
margin_bottom = 554.0

