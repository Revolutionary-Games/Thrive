margin_bottom = 59.0
text = "To unlock the editor and reproduce you need to gather Ammonia (Orange Cloud) and Phosphate (Purple Cloud)."

[node name="HelpBox4" parent="MicrobeHUD/CanvasLayer/HelpScreen/ScrollContainer/HBoxContainer/LeftColumn" index="3" instance=ExtResource( 73 )]
margin_top = 218.0
margin_bottom = 320.0

